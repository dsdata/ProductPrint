<!-- This File is important for all extensions with Magento Backend Integration, 
     it prevented a 500 / access denied error after new installation of extensions 
-->

<config>
	<acl>
		<resources>
			<admin>
				<children>
					<system>
						<children>
							<config>
								<children>
									<productprint translate="title" module="productprint">
										<title>ProductPrint</title>
									</productprint>
								</children>
							</config>
						</children>
					</system>
				</children>
			</admin>
		</resources>
	</acl>
</config>
